# -*- coding: utf-8 -*-
import sqlite3 as lite
import os
dstroot="C:\\temp_read_Excel"
os.chdir(dstroot)
con = lite.connect('database.db')
with con:
    cur=con.cursor()
    search="SELECT * FROM excel_content WHERE Cell_Data LIKE '%陰井人孔%' and Cell_Data LIKE '%孔%'"
    for result in con.execute(search):
        print result[5]
        print result[1:4]
